import java.util.*;
import java.lang.*;
import java.io.*;

class Solution
{                                                      //IDEA: we start add prefix sum in HashSet if we found our value of prefix sum again in HashSet then we can say that
                                                           // subarray weith zero sum exist.
    public static void main (String[] args) 
    {
        int arr[] = new int[]{5, 3, 9, -4, -6, 7, -1};
        int n = arr.length;
        
        System.out.println(ZeroSumSubarray(arr, n));
        
    }
    
    static int ZeroSumSubarray(int arr[], int n)
    {
        Set<Integer> us = new HashSet<Integer>();
        int prefix_sum=0;
        us.add(0);
        for(int i=0;i<n;i++)
        {
            prefix_sum+=arr[i];
            if(us.contains(prefix_sum))
            {
                return 1;
            }
            us.add(prefix_sum);
        }
        return 0;
       
    }
}
